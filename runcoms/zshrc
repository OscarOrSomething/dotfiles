alias msfconsole="msfconsole -x \"db_connect msf@msf\""

# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

alias ls='ls --group-directories-first --color=auto -N'

stty -ixon

# export TERM='xterm-256color'
